	float: right;
}

input[type=text], select {
  width: 11.2%;
  margin: 8px 0;
  display: inline-block;
  box-sizing: border-box;
}

input[type=submit] {
  width: 20%;
  background-color: #4CAF50;
  color: white;
  padding: 14px 20px;
  margin: 8px 0;
  border: none;
  border-radius: 4px;
  cursor: pointer;
}

input[type=submit]:hover {
  background-color: #45a049;
}

div.SignUpSheet {
  border-radius: 5px;
  background-color: #f2f2f2;
  padding: 20px;
}